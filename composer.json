{
    "name": "prestaflow/prestashop",
    "description": "PrestaFlow example",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "Tests/"
        }
    },
    "scripts": {
        "prestaflow:base": "./vendor/prestaflow/php-library/bin/prestaflow run",
        "prestaflow:default": "@prestaflow:base ./Tests",
        "prestaflow:json": "./vendor/prestaflow/php-library/bin/prestaflow run --output=JSON ./Tests",
        "prestaflow:json:file": "./vendor/prestaflow/php-library/bin/prestaflow run --output=JSON ./Tests -f prestaflow/results.json"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "prestaflow/php-library": "^1.0"
    }
}
